{
    "style": "base",
    "extensions": [],
    "fixities": [],
    "formatting": {
        "op": {
            ",": {
                "force-linebreak": false,
                "spaces": "after",
                "linebreaks": "before"
            },
            "default": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "before"
            },
            ". in expression": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "before"
            },
            ". in type": {
                "force-linebreak": false,
                "spaces": "after",
                "linebreaks": "after"
            },
            "record": {
                "force-linebreak": false,
                "spaces": "after",
                "linebreaks": "after"
            }
        },
        "group": {
            "(# in pattern": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "after"
            },
            "default": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "after"
            },
            "(# in expression": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "after"
            }
        },
        "layout": {
            "infix-app": "try-oneline",
            "if": "try-oneline",
            "import-spec-list": "try-oneline",
            "con-decls": "try-oneline",
            "declaration": "try-oneline",
            "app": "try-oneline",
            "let": "try-oneline",
            "record": "try-oneline",
            "type": "try-oneline",
            "export-spec-list": "try-oneline",
            "list-comp": "try-oneline"
        },
        "penalty": {
            "overfull": 10,
            "indent": 1,
            "overfull-once": 200,
            "max-line-length": 80,
            "linebreak": 90
        },
        "indent": {
            "deriving": 2,
            "if": "indent-by 3",
            "let-binds": "align",
            "import-spec-list": "indent-by 2",
            "onside": 2,
            "where": 2,
            "typesig": "indent-by 2",
            "do": "align",
            "app": "indent-by 2",
            "case": "indent-by 2",
            "let-in": "align",
            "where-binds": "indent-by 2",
            "let": "indent-by 2",
            "export-spec-list": "indent-by 2",
            "multi-if": "indent-by 2",
            "class": "indent-by 2"
        },
        "align": {
            "let-binds": false,
            "where": false,
            "matches": false,
            "limits": [
                10,
                25
            ],
            "case": false,
            "import-module": false,
            "import-spec": false,
            "class": false,
            "record-fields": false
        },
        "options": {
            "sort-pragmas": true,
            "flexible-oneline": false,
            "decl-no-blank-lines": [],
            "split-language-pragmas": false,
            "align-sum-type-decl": false,
            "sort-import-lists": true,
            "preserve-vertical-space": false,
            "sort-imports": true
        }
    },
    "language": "Haskell2010"
}
